    "# Load libraries\n",
    "import requests\n",
    "import json\n",
    "import pandas as pd\n",
    "\n",
    "\n",
    "url = \"https://api.openf1.org/v1/drivers?\"\n",
    "Methods= ('drivers?')\n",
    "\n",
    "response = requests.request(\"GET\", url, data=payload)\n",
    "data = response.text\n",
    "parse_json= json.loads(data)\n",
    "df = pd.DataFrame(parse_json)\n",
    "df\n",
    "\n"
   ]
